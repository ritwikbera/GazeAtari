{
	"game":"alien", 
	"mode":"overfit",
	"task":"actionpred",
	"arch":{
		"type":"AtariPolicyFull"
	},
	"loss_fn":{
		"type":"CrossEntropyLoss",
		"args":{
			"reduction":"mean"
		}
	},
	"epochs":40, 
	"optimizer":{
		"type":"Adam",
		"args":{
			"lr":5e-3
		}
	},
	"dataloader":{
		"type": "AtariMSDL",
		"args":{
			"path":"dataset/alien/overfit/",
			"batch_size":4
		}
	},
	"log_dir":"experiments/logs/",
	"checkpoint_dir":"experiments/checkpoint/",
	"log_interval":100, 
	"n_gpu":2,   
	"resume":0,
	"ckpt_path":"experiments/checkpoint/actionmodel.pth"
}